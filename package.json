{
  "name": "@amiearth/mask-email",
  "version": "1.0.2",
  "description": "A lightweight TypeScript library for masking email addresses to protect user privacy",
  "main": "index.js",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "publish:patch": "npm version patch && npm publish --access public",
    "publish:minor": "npm version minor && npm publish --access public",
    "publish:major": "npm version major && npm publish --access public"
  },
  "keywords": [
    "email",
    "mask",
    "privacy",
    "security",
    "typescript",
    "hide",
    "obfuscate",
    "protect"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/earth774/mask-email.git"
  },
  "bugs": {
    "url": "https://github.com/earth774/mask-email/issues"
  },
  "homepage": "https://github.com/earth774/mask-email#readme",
  "type": "module",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^3.2.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
